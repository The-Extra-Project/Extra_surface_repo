<env>
  <datasets>
    <austin>
      <plot_lvl>1</plot_lvl>
      <do_process>true</do_process>
      <datatype>files</datatype>
      <dim>3</dim>
      <ndtree_depth>1</ndtree_depth>
      <bbox>-50x50:-50x50:-50x50</bbox>

      <max_ppt>10000</max_ppt>

      <pscale>0.01</pscale>
      <nb_samples>1</nb_samples>
     
      <lambda>0.1</lambda>
      <max_opt_it>50</max_opt_it>
      <StorageLevel>MEMORY_AND_DISK</StorageLevel>
      <!-- Meta paramexters -->
      <do_process>true</do_process>
      <do_expand>false</do_expand>
      <dump_debug>true</dump_debug>
      
    </austin>
  </datasets>
</env>



